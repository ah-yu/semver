{
  "name": "@buji/semver",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "shx rm -rf dist/*",
    "dist": "npm run clean && npm run build",
    "test": "mocha -r ts-node/register src/**/*.spec.ts --exit",
    "coverage": "nyc --reporter=lcov --reporter=text --reporter=text-summary npm run test",
    "report-coverage": "nyc report --reporter=lcov > coverage.lcov && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ah-yu/semver.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ah-yu/semver/issues"
  },
  "homepage": "https://github.com/ah-yu/semver#readme",
  "devDependencies": {
    "@buji/tsconfig": "0.0.1",
    "@types/semver": "^6.0.0",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^13.1.0",
    "chai": "^4.2.0",
    "codecov": "^3.5.0",
    "coveralls": "^3.0.3",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "prettier": "^1.17.1",
    "shx": "^0.3.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "semver": "^7.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
